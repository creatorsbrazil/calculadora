{
  "expo": {
    "name": "Calculadora",
    "slug": "Calculadora",
    "version": "1.1.0",
    "orientation": "default",
    "icon": "./assets/ic_launcher.png",
    "splash": {
      "image": "./assets/ic_launcher.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "package": "com.impactro.calculadora",
      "versionCode": 10,
      "permissions":[],
      "googleServicesFile": "./google-services.json"
    }
  }
}
